            <Label x:Name="Label5"/>
            <ProgressBar x:Name="Bar5" Progress="0" />
            <Label x:Name="Label6"/>
            <ProgressBar x:Name="Bar6" Progress="0" />
            <Label x:Name="Label7"/>
            <ProgressBar x:Name="Bar7" Progress="0" />
            <Label x:Name="Label8"/>
            <ProgressBar x:Name="Bar8" Progress="0" />
            <Label x:Name="Label9"/>
            <ProgressBar x:Name="Bar9" Progress="0" />
            <Label x:Name="Label10"/>
            <ProgressBar x:Name="Bar10" Progress="0" />

            <Button Text="Modificar Barras" Clicked="Modificar"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>